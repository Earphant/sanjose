<?xml version="1.0" encoding="utf-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
xmlns="http://java.sun.com/xml/ns/javaee"
xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" version="2.5">
	<servlet>
		<servlet-name>community</servlet-name>
		<servlet-class>sanjose.CommunityServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>home</servlet-name>
		<servlet-class>sanjose.HomeServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>signin</servlet-name>
		<servlet-class>sanjose.SigninServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>signout</servlet-name>
		<servlet-class>sanjose.SignoutServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>signup</servlet-name>
		<servlet-class>sanjose.SignupServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>tools</servlet-name>
		<servlet-class>sanjose.ToolsServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>community</servlet-name>
		<url-pattern>/community/*</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>home</servlet-name>
		<url-pattern>/</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>signin</servlet-name>
		<url-pattern>/tools/signin</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>signout</servlet-name>
		<url-pattern>/tools/signout</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>signup</servlet-name>
		<url-pattern>/tools/signup</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>tools</servlet-name>
		<url-pattern>/tools</url-pattern>
	</servlet-mapping>
	<welcome-file-list>
		<welcome-file>index.html</welcome-file>
		<welcome-file>index.css</welcome-file>
		<welcome-file>index.js</welcome-file>
	</welcome-file-list>
</web-app>
